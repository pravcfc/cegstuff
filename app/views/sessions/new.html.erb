<h2>Sign In</h2>
<%= form_for :session, url: sessions_path do |f| %>

 <h4><%= flash.now[:error] %></h4><br /><br />

  <h4><%= notice %></h4><br /><br />

  
<%= f.label :email %>
<%= f.text_field :email %><br /><br />

<%= f.label :password %>
<%= f.password_field :password %><br /><br />

<%= f.submit "SignIn" %>

<% end %>
